    "    shannon_summary_cos <- rbind(shannon_summary_cos, data.frame(Strain = strain, Shannon_Index = shannon_index))\n",
    "    df_strata_counts <- df_strata_counts[, c(\"Stratum\", \"Count\", \"pi\", \"Total\", \"pi_ln_pi\", \"shannon_index\")]\n",
    "    \n",
