    "# _data_MQS: This summarizes the scores in the corresponding sample and their MQScore value.\n",
    "\n",
    "    write.csv(shannon_summary_cos, file = \"Data/CSV_IQ/1_COS_shannon_index_summary.csv\", row.names = FALSE)\n"
   ]
